bool isValid(char* s) {
    int n = strlen(s);
    char stack[n];
    int top = -1;
    for(int i = 0;i<n;i++){
        if(s[i] == '(' || s[i] == '[' || s[i] == '{'){
            stack[++top] = s[i];
        }
        else{
            if(top == -1 || (stack[top] == '(' && s[i] != ')') || (stack[top] == '[' && s[i] != ']') || (stack[top] == '{' && s[i] != '}')){
                return false;
            }
            top--;
        }
    }
    if(top == -1){
        return true;
    }
    else{
        return false;
    }
}
